<Window x:Name="window" x:Class="Laboratorium2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laboratorium2"
        mc:Ignorable="d"
        Title="MainWindow" Height="455" Width="800" KeyDown="Window_KeyDown">
    <Grid x:Name="grid" Margin="0,0,0,0">
        <Canvas x:Name="canvas" Width="600" Height="200" Margin="96,77,96,147" Background="#FFB2B2B2">
            <Rectangle x:Name="rect" Fill="Blue" HorizontalAlignment="Left" Height="25" Stroke="Black" VerticalAlignment="Top" Width="25"/>
            <Rectangle x:Name="enemy" Fill="Red" HorizontalAlignment="Left" Height="25" Stroke="Black" VerticalAlignment="Top" Width="25"/>
        </Canvas>
        <Image HorizontalAlignment="Left" Height="70" Margin="0,354,0,0" VerticalAlignment="Top" Width="792" Panel.ZIndex="-1" Source="ekwipunek.png" Stretch="Fill"/>
        <Image HorizontalAlignment="Left" Height="354" VerticalAlignment="Top" Width="792" Panel.ZIndex="-1" Stretch="Fill" Source="bck.png"/>
        <Label x:Name="inRange" Content="Label" HorizontalAlignment="Left" Margin="121,20,0,0" VerticalAlignment="Top" Width="170"/>
    </Grid>
</Window>
